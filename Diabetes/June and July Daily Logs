SELECT * FROM JUNE_BG
UNION 
SELECT * FROM JULY_BG;

/* Finding logs where I was low, but didn't record that I felt low */

SELECT Log_Day
, Log_Time
, Bolus
, Blood_Sugar
, Carbs
, Notes
FROM (
  SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
  FROM JUNE_BG
  UNION 
  SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
  FROM JULY_BG
) AS Combined_Months
WHERE Blood_Sugar <= 100
AND Notes NOT LIKE '%Hypo Feeling%';

/* Wanting to find out if there's a correlation for when my blood sugar is high, and if I seem to be snacking/hungry as a symptom */

SELECT Log_Day
, Log_Time
, Bolus
, Blood_Sugar
, Carbs
, Notes
, (CASE
    WHEN Blood_Sugar >= 200 AND Notes LIKE "%Snack%" THEN 'Yes'
    ELSE 'No'
END) AS High_Snacks
FROM (
  SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
  FROM JUNE_BG
  UNION ALL
  SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
  FROM JULY_BG
) AS Combined_Months
ORDER BY Log_Day ASC;

WITH High_Snacks_Counts AS (
    SELECT
        COUNT(*) AS TotalRows,
        SUM(CASE
                WHEN Blood_Sugar >= 200 AND Notes LIKE "%Snack%" THEN 1
                ELSE 0
            END) AS High_Snacks_Rows
    FROM (
        SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
        FROM JUNE_BG
        WHERE Blood_Sugar >= 200
        UNION ALL
        SELECT Log_Day, Log_Time, Bolus, Blood_Sugar, Carbs, Notes
        FROM JULY_BG
        WHERE Blood_Sugar >= 200
    ) AS Combined_Months
)
SELECT TotalRows
, High_Snacks_Rows
, ROUND((High_Snacks_Rows * 100.0 / TotalRows), 2) AS Percentage
FROM High_Snacks_Counts;
