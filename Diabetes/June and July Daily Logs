CREATE TABLE JUNE_BG
(id INTEGER PRIMARY KEY AUTOINCREMENT
, Log_Day DATE
, Log_Time TIME
, Basal REAL
, Bolus REAL
, Blood_Sugar INTEGER
, Carbs INTEGER
, Activity_Duration INTEGER -- Measured in Minutes
, Notes VARCHAR(500));

